{
  "name": "recipes-bierhall",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "critical": "sass --style compressed --no-source-map src/sass/critical.scss:src/critical/critical.css",
    "watch:critical": "sass --style compressed --no-source-map --watch src/sass/critical.scss:src/critical/critical.css",
    "watch:eleventy": "rm -rf prod && ELEVENTY_RUN_MODE=serve npx --node-options='--experimental-require-module' @11ty/eleventy --serve",
    "serve": "npm-run-all --parallel watch:eleventy watch:critical",
    "build": "rm -rf prod && ELEVENTY_RUN_MODE=build npx --node-options='--experimental-require-module' @11ty/eleventy"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "type": "module",
  "devDependencies": {
    "@11ty/eleventy": "^3.1.2",
    "eleventy-plugin-rev": "^2.0.0",
    "eleventy-sass": "^3.0.0-beta.0",
    "html-minifier-terser": "^7.2.0",
    "node-sprite-generator": "^0.10.2",
    "npm-run-all": "^4.1.5",
    "sass": "^1.94.0",
    "terser": "^5.44.1"
  }
}
